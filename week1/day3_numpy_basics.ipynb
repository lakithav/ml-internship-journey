import numpy as np
marks = np.array([55,70,82,90,65])
print(marks)
print(type(marks))

#mathematica operations

#1.Vectorized operations (doing whole operation to the array once)

print(marks+5)
print(marks*10)
print("Mean",np.mean(marks))
print("Max",np.max(marks))
print("Min",np.min(marks))
print("Sum",np.sum(marks))

passed = marks[marks>=60]
print("Passed students:",passed)

average = np.mean(passed)
print("Average of Passed students:",average)
